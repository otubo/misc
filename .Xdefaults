URxvt.perl-ext-common:      default,tabbed,matcher

URxvt.urlLauncher:          firefox
URxvt.matcher.button:       1
URxvt*letterSpace:          -2
URxvt*boldFont:             xft:Monospace:pixelsize=10
URxvt*font:                 xft:Monospace:pixelsize=10
URxvt*borderLess:           false
URxvt*externalBorder:       0
URxvt*internalBorder:       0
URxvt*scrollBar:            false
URxvt*depth:                32
URxvt*background:           [90]#000000
URxvt*foreground:           #ffffff
URxvt*cursorColor:          #ffffff
URxvt*cursorColor2:         #000000
URxvt*cursorBlink:          false
URxvt*inheritPixmap:        false
URxvt*saveLines:            8000
URxvt*scrollTtyOutput:      false
URxvt*scrollTtyKeypress:    true
URxvt*mouseWheelScrollPage: false
! URxvt*cutchars:             `'",;@&*=|?()<>[]{}
URxvt.urlLauncher:          firefox

URxvt.tabbed.tabbar-fg:     8
URxvt.tabbed.tabbar-bg:     0
URxvt.tabbed.tab-fg:        8
URxvt.tabbed.tab-bg:        6

URxvt.keysym.C-Down:        \033[1;3B
!URxvt.keysym.C-Up:          \033[1;3A
!URxvt.keysym.C-Left:        \033[1;3D
URxvt.keysym.C-Right:       \033[1;3C

URxvt*background:           #000000
URxvt*foreground:           #ffffff
! black
URxvt.color0:               #000000
URxvt.color8:               #555555
! red
URxvt.color1:               #AA0000
URxvt.color9:               #FF5555
! green
URxvt.color2:               #00AA00
URxvt.color10:              #55FF55
! yellow
URxvt.color3:               #AA5500
URxvt.color11:              #FFFF55
! blue
URxvt.color4:               #0000AA
URxvt.color12:              #5555FF
! magenta
URxvt.color5:               #AA00AA
URxvt.color13:              #FF55FF
! cyan
URxvt.color6:               #00AAAA
URxvt.color14:              #55FFFF
! white
URxvt.color7:               #AAAAAA
URxvt.color15:              #FFFFFF

XTerm*metaSendsEscape: true
